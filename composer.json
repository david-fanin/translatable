{
  "name": "spletna-postaja/translatable",
  "description": "Translatable eloquent models.",
  "keywords": ["translatable", "eloquent", "laravel", "model", "translate", "i18n"],
  "license": "MIT",
  "authors": [
    {
      "name": "Anže Časar",
      "email": "anze.casar@gmail.com"
    },
    {
      "name": "Spletna Postaja",
      "email": "developer@spletna-postaja.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/database": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpunit/php-timer": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laraplus\\Data\\TranslatableServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Laraplus\\Data\\": "src/"
    }
